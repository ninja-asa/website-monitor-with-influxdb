[run]
source = webmonitor
branch = True
omit = 
    */__init__.py
    */__main__.py
    */tests/*
    */test_*
    */setup.py
    */conftest.py
    */_version.py
    *main.py
    */mylogger.py

[report]
exclude_lines = 
    def __repr__
    if self\.debug
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    @(abc\.)?abstractmethod
ignore_errors = True

[html]
directory = coverage_html_report